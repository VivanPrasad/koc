[audio]

default_bus_layout="res://Assets/Misc/default_bus_layout.tres"

[autoload]

Transition="*res://Scenes/Autoloads/Transition.tscn"
Audio="*res://Scenes/Autoloads/Audio.tscn"
PlayerStats="*res://Scripts/Singletons/PlayerStats.gd"
